{
    "root": true,
    "rules": {
        "simple-import-sort/imports": 1,
        "simple-import-sort/exports": 1,
        "vue/html-indent": [1, 4],
        "vue/multi-word-component-names": 0
    },
    "extends": ["standard", "prettier", "plugin:vue/vue3-recommended"],
    "plugins": ["simple-import-sort"],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    }
}
